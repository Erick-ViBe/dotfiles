cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - h : yabai -m window --focus west
cmd - l : yabai -m window --focus east

cmd + ctrl - h : yabai -m display --focus west
cmd + ctrl - l : yabai -m display --focus east

cmd + ctrl - a : yabai -m space --create
cmd + ctrl - d : yabai -m space --destroy
cmd + ctrl - e : yabai -m space --focus prev || yabai -m space --focus last
cmd + ctrl - i : yabai -m space --focus next || yabai -m space --focus first

cmd + ctrl - n : yabai -m window --swap west  || $(yabai -m window --display west; yabai -m display --focus west)
cmd + ctrl - m : yabai -m window --swap east || $(yabai -m window --display east; yabai -m display --focus east)

ctrl - e : yabai -m space --balance
ctrl - r : yabai -m space --rotate 90
ctrl - d : yabai -m window --toggle split

ctrl - l : yabai -m window --resize right:20:0 2> /dev/null || yabai -m window --resize left:20:0 2> /dev/null
ctrl - h : yabai -m window --resize right:-20:0 2> /dev/null || yabai -m window --resize left:-20:0 2> /dev/null
ctrl - j : yabai -m window --resize bottom:0:20 2> /dev/null || yabai -m window --resize top:0:20 2> /dev/null
ctrl - k : yabai -m window --resize bottom:0:-20 2> /dev/null || yabai -m window --resize top:0:-20 2> /dev/null
