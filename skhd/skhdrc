cmd + ctrl - j : yabai -m window --focus south
cmd + ctrl - k : yabai -m window --focus north
cmd + ctrl - h : yabai -m window --focus west
cmd + ctrl - l : yabai -m window --focus east

cmd + ctrl - n : yabai -m display --focus west
cmd + ctrl - b : yabai -m display --focus east

cmd + ctrl - a : yabai -m space --create
cmd + ctrl - d : yabai -m space --destroy
cmd + ctrl - e : yabai -m space --focus prev || yabai -m space --focus last
cmd + ctrl - i : yabai -m space --focus next || yabai -m space --focus first

cmd + ctrl - y : yabai -m window --swap west  || $(yabai -m window --display west; yabai -m display --focus west)
cmd + ctrl - t : yabai -m window --swap east || $(yabai -m window --display east; yabai -m display --focus east)

cmd + ctrl - r : yabai -m space --rotate 90
cmd + ctrl - c : yabai -m space --balance
cmd + ctrl - v : yabai -m window --toggle split

cmd + ctrl - u : yabai -m window --resize right:-20:0 2> /dev/null || yabai -m window --resize left:-20:0 2> /dev/null
cmd + ctrl - p : yabai -m window --resize right:20:0 2> /dev/null || yabai -m window --resize left:20:0 2> /dev/null
